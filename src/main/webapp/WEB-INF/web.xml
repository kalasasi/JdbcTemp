<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>UserRegisterServlet</servlet-name>
  	<display-name>UserRegisterServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.controller.UserRegisterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserLoginServlet</servlet-name>
  	<display-name>UserLoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.controller.user.UserLoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>NewUserRegisterServlet</servlet-name>
  	<display-name>NewUserRegisterServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.controller.user.NewUserRegisterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.controller.user.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ResetPasswordServlet</servlet-name>
  	<display-name>ResetPasswordServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.controller.user.ResetPasswordServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserRegisterServlet</servlet-name>
  	<url-pattern>/UserRegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserLoginServlet</servlet-name>
  	<url-pattern>/UserLoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>NewUserRegisterServlet</servlet-name>
  	<url-pattern>/NewUserRegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ResetPasswordServlet</servlet-name>
  	<url-pattern>/ResetPasswordServlet</url-pattern>
  </servlet-mapping>
</web-app>
